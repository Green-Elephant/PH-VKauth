{
  "manifest_version": 2,
  "name": "PornHub обход авторизации ВК",
  "version": "1.1.0",
  "description": "Позволяет посещать PornHub без авторизации ВК. Работает при входе в инкогнито. Убирает рекламу",
  "author": "David Lukava",
  "icons": {
    "16": "img/icon-128.png",
    "48": "img/icon-128.png",
    "128": "img/icon-128.png"
  },
  "content_scripts": [
    {
      "matches" : ["*://*.pornhub.com/*"],
      "run_at" : "document_start",
      "js": ["content/adBlock.js"]
    }
  ],
  "homepage_url": "https://vk.com/ge_tt",
  "permissions":["*://*.pornhub.com/*"]
}
